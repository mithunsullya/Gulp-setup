{% extends "node.html.twig" %}

{% block content %}
  {{ attach_library("sdnn_theme/webinar-carousel-card") }}
  <h4 class="title">
    <a href="{{ content.field_webinar_cta[0]['#url'] }}" rel="bookmark" target="_blank">{{ label }}</a>
  </h4>
  <div class="subtitle">
    {% if content.field_course_date %}
      {{ content.field_course_date }}
    {% endif %}
    {% if content.field_webinar_cta %}
      {{ content.field_webinar_cta }}
    {% endif %}
  </div>
{% endblock %}
