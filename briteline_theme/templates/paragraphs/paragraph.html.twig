{#
/**
 * @file
 * Default theme implementation to display a paragraph.
 */
#}

{% set classes = [
  'paragraph',
  'paragraph--type--' ~ paragraph.bundle|clean_class,
] %}

{% block paragraph %}
  <div{{ attributes.addClass(classes) }}>
      {% block content %}
        {{ content }}
      {% endblock %}
  </div>
{% endblock %}
